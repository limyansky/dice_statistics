Cell[BoxData[
 RowBox[{
  RowBox[{"modeFrac", "[", "number_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"N", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"Count", "[", 
         RowBox[{"#", ",", " ", 
          RowBox[{"FromDigits", "[", 
           RowBox[{"Commonest", "[", "#", "]"}], "]"}]}], "]"}], "&"}], "@", 
       RowBox[{"Pick", "[", 
        RowBox[{"finalTotal", ",", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"#", "==", "number"}], "&"}], ",", "firstTwoTotal"}], 
          "]"}]}], "]"}]}], ")"}], "/", "\[IndentingNewLine]", 
     RowBox[{"(", 
      RowBox[{"Length", "[", 
       RowBox[{"Pick", "[", 
        RowBox[{"finalTotal", ",", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"#", "==", "number"}], "&"}], ",", "firstTwoTotal"}], 
          "]"}]}], "]"}], "]"}], ")"}]}], ",", "\[IndentingNewLine]", "3"}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
