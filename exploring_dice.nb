Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"exactDice", "[", "sides_", "]"}], ":=", 
  RowBox[{
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"n", "=", "1"}], 
    RowBox[{"Abs", "[", "sides", "]"}]], 
   RowBox[{
    FractionBox["1", 
     RowBox[{"Abs", "[", "sides", "]"}]], 
    RowBox[{"x", "^", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Sign", "[", "sides", "]"}], "*", "n"}], 
      ")"}]}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"diceList", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"6", ",", "40"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ndice", "=", 
   RowBox[{"Length", "@", "diceList"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"diceDistribution", " ", "=", " ", 
   RowBox[{"Product", "[", 
    RowBox[{
     RowBox[{"exactDice", "[", "d", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"d", ",", " ", "diceList"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"probs", " ", "=", " ", 
   RowBox[{"CoefficientList", "[", 
    RowBox[{
     RowBox[{"diceDistribution", "*", 
      SuperscriptBox["x", 
       RowBox[{"Abs", "[", 
        RowBox[{"Total", "[", "diceList", "]"}], "]"}]]}], ",", " ", "x"}], 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"probs", " ", "=", " ", 
   RowBox[{"DeleteCases", "[", 
    RowBox[{"probs", ",", " ", "0"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"values", " ", "=", " ", 
   RowBox[{"Exponent", "[", 
    RowBox[{"diceDistribution", ",", "x", ",", "List"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListStepPlot", "[", 
  RowBox[{"Thread", "[", 
   RowBox[{"{", 
    RowBox[{"values", ",", " ", "probs"}], "}"}], "]"}], "]"}]}], "Input",
